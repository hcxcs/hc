.container{
  padding: 30rpx 16rpx;
}
.line{
  position: relative;
  padding-bottom: 15rpx;
}
.order-title{
  margin-right: 10rpx;
}
.order-unit{
  margin-left: 5rpx;
  font-size: 26rpx;
}
.ip-ipt{
  width: 80rpx;
  display: inline-block;
  border: 2rpx solid #d3d3d3;
  margin-bottom: -15rpx;
  font-size: 28rpx;
  text-align: center;
}
.apn-ipt{
  margin-bottom: -10rpx;
  padding: 0rpx 5rpx;
  display: inline-block;
  border: 2rpx solid #d3d3d3;
  font-size: 28rpx;
}
.btn-line{
  position: fixed;
  width: 100%;
  bottom: 100rpx;
  text-align: center;
}
.btn-line>button{
  display: inline-block;
    line-height: normal;
    padding: 20rpx 70rpx;
    font-size: 30rpx;
    border: none;
    background-color: #2196F3;
    color: #fff;
}